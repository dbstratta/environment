{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "linterOptions": {
    "exclude": ["node_modules"]
  }
}
